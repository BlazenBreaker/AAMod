<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\references\tModLoader.targets" />
  <PropertyGroup>
    <AssemblyName>AAMod</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <Target Name="BuildMod" AfterTargets="Build">
    <Exec Command="&quot;$(tMLBuildServerPath)&quot; -build $(ProjectDir) -eac $(TargetPath) -define $(DefineConstants) -unsafe $(AllowUnsafeBlocks)" />
  </Target>
  <ItemGroup>
    <Compile Remove="Players\**" />
    <EmbeddedResource Remove="Players\**" />
    <None Remove="Players\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="obj\Debug\net45\AAMod.AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="obj\AAMod.csproj.nuget.dgspec.json" />
    <Content Include="obj\Debug\AAMod.csproj.FileListAbsolute.txt" />
    <Content Include="obj\Debug\net45\AAMod.csproj.FileListAbsolute.txt" />
    <Content Include="obj\project.assets.json" />
  </ItemGroup>
  <ItemGroup>
    <None Include="AAMod.sln" />
    <None Include="bin\Debug\AAMod.dll" />
    <None Include="bin\Debug\AAMod.pdb" />
    <None Include="bin\Debug\BaseMod.dll" />
    <None Include="bin\Debug\Microsoft.Xna.Framework.dll" />
    <None Include="bin\Debug\Microsoft.Xna.Framework.Game.dll" />
    <None Include="bin\Debug\Microsoft.Xna.Framework.Graphics.dll" />
    <None Include="bin\Debug\Microsoft.Xna.Framework.Xact.dll" />
    <None Include="bin\Debug\ReLogic.dll" />
    <None Include="bin\Debug\Terraria.exe" />
    <None Include="obj\AAMod.csproj.nuget.cache" />
    <None Include="obj\AAMod.csproj.nuget.g.props" />
    <None Include="obj\AAMod.csproj.nuget.g.targets" />
    <None Include="obj\Debug\AAMod.csproj.CopyComplete" />
    <None Include="obj\Debug\AAMod.csproj.CoreCompileInputs.cache" />
    <None Include="obj\Debug\AAMod.csprojAssemblyReference.cache" />
    <None Include="obj\Debug\AAMod.dll" />
    <None Include="obj\Debug\AAMod.pdb" />
    <None Include="obj\Debug\DesignTimeResolveAssemblyReferences.cache" />
    <None Include="obj\Debug\DesignTimeResolveAssemblyReferencesInput.cache" />
    <None Include="obj\Debug\net45\AAMod.AssemblyInfoInputs.cache" />
    <None Include="obj\Debug\net45\AAMod.assets.cache" />
